 <style>
      /* Styling untuk efek hover */
      .card:hover {
      transform: translateY(-28px);
      transition: transform 0.7s ease-in-out;
      box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2); /* Tambahkan bayangan saat hover */
  }

  .icon-active path {
      fill: rgb(255, 255, 255);
  }
  
  
  .icon-default path {
      fill: black;
  }
  
  .masonry {
      column-count: 4; /* Changed to 4 columns */
      column-gap: 26px; /* Space between columns */
  }

  .masonry-item {
      break-inside: avoid; /* Prevent items from breaking inside the column */
      margin-bottom: 26px; /* Space between rows */
      transition: transform 0.2s; /* Transisi saat hover */
  }

  .masonry-item:hover {
      transform: scale(1.05); /* Skala gambar saat hover */
  }

  /* Animasi untuk tombol */
  .button-animation {
      transition: background-color 0.5s ease, transform 0.5s ease; /* Transisi untuk perubahan latar belakang */
  }

  .button-animation:active {
      transform: scale(0.95); /* Efek mengecil saat diklik */
  }

  @keyframes fadeIn {
        from {
            opacity: 0;
            transform: translateY(20px); /* Bergeser sedikit ke bawah */
        }
        to {
            opacity: 1;
            transform: translateY(0);
        }
    }

    .fade-in-animation {
        animation: fadeIn 0.5s ease-in-out forwards; /* Menggunakan animasi fadeIn */
    }

    /* Transisi gambar saat hover */
    .project-image:hover {
        transform: scale(1.05); /* Efek membesar saat di-hover */
        transition: transform 0.3s ease; /* Transisi halus */
    }
  
  </style>

